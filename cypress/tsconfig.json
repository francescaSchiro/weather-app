{
	"compilerOptions": {
		"resolveJsonModule": true,
		"moduleResolution": "node",
		"baseUrl": "../node_modules",
		"allowSyntheticDefaultImports": true,
		"target": "es6",
		"lib": [
			"es6",
			"dom",
			"esnext"
		],

		// "target": "es6",
    // "lib": ["es6", "dom"],
		"types": [
			"cypress",
			"node"
		]
	},

	"noEmit": true,
	"isolatedModules": false,
	"include": [
		"../node_modules/cypress",
		"**/*.ts",
		"../cypress.d.ts"
	],
	
}