{
	"extends": "../tsconfig.json",
	"compilerOptions": {
		"noEmit": true,
		"target": "es5",
    "lib": ["es5", "dom"],
		"types": [
			"cypress",
			"node"
		]
	},
	"isolatedModules": false,
	"include": [
		"../node_modules/cypress",
		"**/*.ts",
		"../cypress.d.ts"
	],
	
	"resolveJsonModule": true,
	"moduleResolution": "node",
	"baseUrl": "../node_modules",
	"allowSyntheticDefaultImports": true,
	"target": "es6",
	"lib": [
		"es6",
		"dom",
		"esnext"
	],
}
